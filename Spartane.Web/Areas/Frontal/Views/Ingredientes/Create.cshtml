		function Fill_Datos_Generales()
        {
            $.ajax({
                url: "@Url.Content("~/Frontal/Ingredientes/Get_Datos_Generales")?Id=" + $("#ReferenceClave").val(),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                                        $('#Clave').val(result.data.Clave);
                    $('#Nombre_Ingrediente').val(result.data.Nombre_Ingrediente);
                    var control = $('#Clasificacion');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Clasificacion).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Clasificacion).trigger('change');
                    }, 3000);
                    $('#Imagen').val(result.data.Imagen);
                    $('#Subgrupo').val(result.data.Subgrupo);
                    $('#Cantidad_sugerida').val(result.data.Cantidad_sugerida);
                    $('#Unidad').val(result.data.Unidad);
                    $('#Peso_bruto_redondeado_g').val(result.data.Peso_bruto_redondeado_g);
                    $('#Peso_neto_g').val(result.data.Peso_neto_g);
                    $('#Nombre_sistema_anterior').val(result.data.Nombre_sistema_anterior);
                    Detalle_Caracteristicas_IngredienteDataDataTable = result.Caracteristicas_Ingrediente;
                    showDetalle_Caracteristicas_IngredienteGrid(result.Caracteristicas_Ingrediente);
                    $('#Detalle_Caracteristicas_IngredienteGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Caracteristicas_IngredienteGrid .gridDatePicker').attr("readonly", "");

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Datos_Generales(newId) {
			var data = new FormData();
			data.append('Clave', newId);
			                data.append('Nombre_Ingrediente', $('#Nombre_Ingrediente').val());
                data.append('Clasificacion', $('#Clasificacion').val());
                data.append('Imagen', $('#hdnAttachedImagen').val());
                data.append('ImagenFile', $('#ImagenFile')[0].files[0]);
                data.append('ImagenRemoveAttachment', $('#hdnRemoveImagen').val());
                data.append('Subgrupo', $('#Subgrupo').val());
                data.append('Cantidad_sugerida', $('#Cantidad_sugerida').val());
                data.append('Unidad', $('#Unidad').val());
                data.append('Peso_bruto_redondeado_g', $('#Peso_bruto_redondeado_g').val());
                data.append('Peso_neto_g', $('#Peso_neto_g').val());
                data.append('Nombre_sistema_anterior', $('#Nombre_sistema_anterior').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Ingredientes/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
                    					    SendDetalle_Caracteristicas_Ingrediente(null, newId);

					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}


